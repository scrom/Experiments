bind 0.0.0.0
protected-mode no

user default off
user mvta on >${REDIS_PWD} ~* +get +set +del +exists +incr +mget +mset +strlen +keys +keys +hgetall +info +select +scan +multi +exec +discard

save 60 1
appendonly yes
appendfsync everysec

# Logging
loglevel warning

#disable dangerous commands
#rename-command FLUSHALL ""
#rename-command FLUSHDB ""
#rename-command CONFIG ""
#rename-command SHUTDOWN ""